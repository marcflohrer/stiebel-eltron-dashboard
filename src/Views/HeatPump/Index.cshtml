        RuntimeVaporizerDefrostDeltaYear = Model.GetYears().Select(x => x.RuntimeVaporizerDefrostDelta).ToList().ToJson(); 
        ReheatingStages1StartYear = Model.GetYears().Select(x => x.ReheatingStages1Start).ToList().ToJson(); 
        ReheatingStages1EndYear = Model.GetYears().Select(x => x.ReheatingStages1End).ToList().ToJson(); 
