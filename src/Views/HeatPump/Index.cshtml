        ReheatingStages2DeltaYear = Model.GetYears().Select(x => x.ReheatingStages2Delta).ToList().ToJson(); 
        DefrostTimeStartYear = Model.GetYears().Select(x => x.DefrostTimeStart).ToList().ToJson(); 
        DefrostTimeEndYear = Model.GetYears().Select(x => x.DefrostTimeEnd).ToList().ToJson(); 
        DefrostTimeDeltaYear = Model.GetYears().Select(x => x.DefrostTimeDelta).ToList().ToJson(); 
        DefrostStartsStartYear = Model.GetYears().Select(x => x.DefrostStartsStart).ToList().ToJson(); 
        DefrostStartsEndYear = Model.GetYears().Select(x => x.DefrostStartsEnd).ToList().ToJson(); 
        DefrostStartsDeltaYear = Model.GetYears().Select(x => x.DefrostStartsDelta).ToList().ToJson();

        PerformanceFactorTotal = Model.Where(m => m.DateUpdated.CompareTo(newestRecord) == 0).Select(x => x.PerformanceFactorTotal()).Take(1).ToList().ToJson();
    }
